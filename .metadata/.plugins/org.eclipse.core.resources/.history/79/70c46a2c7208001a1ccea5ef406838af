package homework4;

public class Random {

	public static void main(String[] args) {
		int[] number = new int[15]; 
		int count = 0;
		String index = "";
		for(int i = 0 ; i < number.length; i++) {
			int random = (int)(Math.random() * 11 + 0);
			random = number[i];
			System.out.print(number[i] + " ");
			if(number[i] == 5) {
				count++;
				index = index + i;
			}	
		}
		System.out.println(" ");
		if(count != 0) {
			System.out.print( "There are" + " " + count +" of Number 5 found in index as below." );
			System.out.println(" ");
			System.out.println("Index:"+ " "+ index);
		}else {
			System.out.println("Number 5 not found");
		}
		System.out.println(" ");
		
	}

}
