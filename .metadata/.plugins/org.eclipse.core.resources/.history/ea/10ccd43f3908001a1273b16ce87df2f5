
public class Answer2_random {

	public static void main(String[] args) {
		
	      int number[] = new int[15];
	      String index = "";
	      int count =0;
	      for (int i = 0; i < number.length; i++) {
	        int random = (int)(Math.random() * 11 + 0);
	        if(random ==5) {
	        	count++;
	        	index = index + 1;
	        }
	        System.out.print(random + " ");

	      }
	      System.out.println(" ");
	      System.out.print(count);
	}

}
