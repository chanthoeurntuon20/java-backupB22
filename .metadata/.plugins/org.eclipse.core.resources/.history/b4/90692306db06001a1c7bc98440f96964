
public class lowest {

	public static void main(String[] args) {
		String[] employee = {"Doung","Chan","Minea","Mesa","Romdoul","Veha","Por","Kanika"};
		int[] salary = { 560, 560, 500, 500, 190, 190, 560,700};
		String lowem = employee[0];
		int lowest = salary[0];
		for(int i = 0; i < salary.length; i++) {
			for(int j = 1; j < salary.length; i++) {
				if(lowest > salary[i]) {
					lowest =  salary[i];
					lowem = employee[i];
			 }
			}
			
	}
		System.out.println(lowest + lowem);
}
}