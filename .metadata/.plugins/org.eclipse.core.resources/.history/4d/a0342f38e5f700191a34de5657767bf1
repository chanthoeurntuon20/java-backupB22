package homewok3;

public class Answer3 {

	public static void main(String[] args) {
		int count = 0;
		int agrs = 0;
		int sum = 0;
		int result = 0;
		int i = 1;
		while(i <= 50) {
			if(i % 2 != 0) {
				count++;
				sum += i;
			}
			i++;
		}
		agrs = sum / count;
		result = sum/agrs;
		System.out.println(result);
	}

}
